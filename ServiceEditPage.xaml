<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Proiect_MDP_Mobile.ServiceEditPage"
             Title="Edit Service">
    <StackLayout>
        <Label Text="Type" />
        <Entry x:Name="entryType" />

        <Label Text="Description" />
        <Entry x:Name="entryDescription" />

        <Label Text="Date" />
        <DatePicker x:Name="datePickerDate" />

        <Label Text="Racket" />
        <Picker x:Name="pickerRacket" />

        <Button Text="Save Changes" Clicked="OnSaveChangesClicked" />
        <Button Text="Cancel" Clicked="OnCancelClicked" />
    </StackLayout>
</ContentPage>