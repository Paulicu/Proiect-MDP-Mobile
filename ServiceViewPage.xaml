<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Proiect_MDP_Mobile.ServiceViewPage"
             Title="View Service">
    <StackLayout>
        <Label Text="Type" />
        <Label x:Name="labelType" />

        <Label Text="Description" />
        <Label x:Name="labelDescription" />

        <Label Text="Date" />
        <Label x:Name="labelDate" />

        <Label Text="Racket" />
        <Label x:Name="labelRacket" />

        <Button Text="Edit" Clicked="OnEditServiceClicked" />
        <Button Text="Back to List" Clicked="OnBackToListClicked" />
    </StackLayout>
</ContentPage>